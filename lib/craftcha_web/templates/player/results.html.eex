<div class="block check-result">
<h3>Last check result</h3>
<%= for {r, index}  <- Enum.with_index(@result) do %>
<div class="result-line">
    <div class="result-line-level">Level <%= index %></div>
    <div class="result-line-status <%= elem(r, 0) %>">
    <%= elem(r, 0) %>
    </div>
    <div class="result-line-message">
    <%= elem(r, 1) %>
    </div>
</div>
<% end %>
</div>
